game:GetService("StarterGui"):SetCore("SendNotification",{
                Title = "PIGGY SCRIPT";
                Text = "If your seeing this the script is loading...";
                Duration = 3.5;
            })



local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("PIGGY!", "BloodTheme")

local Tab1 = Window:NewTab("MAIN")
local Section = Tab1:NewSection("Local")

Section:NewButton("Item Giver", "PIGGY ITEM GIVER", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/9fyyVTSg"))()
end)

Section:NewLabel("HUB")

Section:NewButton("Master PIGGY Hub", "Keyless ", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/lolpoppyus/Roblox-Lua/master/Piggy'),true))()
end)

Section:NewButton("Vynixius"), "HUB", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Vynixius/main/Piggy/Loader.lua"))()
end)




Section:NewLabel("PLAYER")


Section:NewButton("Hitbox", "Dont know if this might work?", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/1aCpdC6a'))()
end)

Section:NewButton("Piggy Flying", "Flying GUI", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/ZCmezffM'))()
end)

Section:NewButton("Teleport - Player", "Teleport to other players!", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/YNVbeqPy'))()
end)

Section:NewButton("Noclip", "Go though things", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/2eaRwyPe'))()
end)

Section:NewButton("Infinite Jump", "Jump forever!", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/4MW9Dbm8'))()
end)

Section:NewButton("ESP", "Find other people", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/hpMC6ULU'))()
end)




Section:NewButton("WalkSpeed 25", "GO FAST?", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 25
end)

Section:NewButton("WalkSpeed 50", "GO FAST?", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
end)

Section:NewButton("WalkSpeed 100", "GO FAST?", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
end)

Section:NewButton("WalkSpeed 200", "GO FAST?", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 200
end)

Section:NewButton("JumpPower 25", "GO HIGH?", function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 25
end)

Section:NewButton("JumpPower 50", "GO HIGH?", function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
end)

Section:NewButton("JumpPower 100", "GO HIGH?", function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100
end)

Section:NewButton("JumpPower 200", "GO HIGH?", function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 200
end)

Section:NewButton("Reset JP/WS", "Reset Walkspeed/JumpPower", function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)


























